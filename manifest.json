{
  "manifest_version": 3,
  "name": "Snooze Tab",
  "version": "1.0",
  "description": "Snooze tabs and have them reopen automatically later.",
  "permissions": ["tabs", "alarms", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "Tab Snooze"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
